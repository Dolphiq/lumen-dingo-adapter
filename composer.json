{
    "name": "dolphiq/lumen-dingo-adapter",
    "description": "Using Dingo + JWT in your Lumen Based Application with no pain",
    "type": "library",
    "version": "2.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zeek\\LumenDingoAdapter\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/hashing": "^7.0 || ^8.0",
        "tymon/jwt-auth": "^1.0.2",
        "illuminate/cookie": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/http": "^7.0 || ^8.0",
        "dingo/api": "^3"
    },
    "authors": [
        {
            "name": "Krisan Alfa Timur",
            "email": "krisan47@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-dist": true
